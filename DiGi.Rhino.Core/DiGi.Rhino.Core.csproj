<Project Sdk="Microsoft.NET.Sdk">
	
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <Version>1.0</Version>
    <OutputPath>..\bin\</OutputPath>
    <Title>DiGi.Rhino.Core</Title>
